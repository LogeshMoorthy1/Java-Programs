package com.workout2nJava;

public class PrimeNumber {

	public static void main(String[] args) {
int num=96;
boolean prime =false;
for (int i = 0; i <num; i++) {
	if(i%2==0);
	prime=false;
	
}
if(prime==false) {
	System.out.println("it is not a prime numeber");
}
else {
	System.out.println("it is a prime number");
}
	}

}
